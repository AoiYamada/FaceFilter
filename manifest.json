{
  "name": "FaceFilter",
  "version": "1.0",
  "description": "Remove people's face in browser",
  "web_accessible_resources": ["asset/*/*", "models/*", "lib/*", "content.js"],
  "background": {
    "scripts": ["lib/face-api.min.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["lib/jquery-3.4.1.min.js", "lib/face-api.min.js", "content.js"]
    }
  ],
  "permissions": ["activeTab"],
  "icons": { "16": "icons/16.png", "32": "icons/32.png", "128": "icons/128.png" },
  "manifest_version": 2
}
